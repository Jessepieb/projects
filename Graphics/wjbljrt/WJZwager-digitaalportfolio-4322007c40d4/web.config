<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By"/>
                <remove name="Server"/>
                <add name="X-XSS-Protection" value="1; mode=block"/> 
                <add name="X-Content-Type-Options" value="nosniff "/>
                <add name="X-Frame-Options" value="DENY"/>
                <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains" />
            </customHeaders>
        </httpProtocol>
        <staticContent>
            <mimeMap fileExtension=".mtl" mimeType="text/plain" />
			<mimeMap fileExtension=".obj" mimeType="text/plain" />
			<mimeMap fileExtension=".dds" mimeType="image/x-dds" />
            <remove fileExtension=".json"/>
            <mimeMap fileExtension=".json" mimeType="application/x-javascript"/>
            <mimeMap fileExtension=".datagz" mimeType="application/binary"/>
            <mimeMap fileExtension=".jsgz" mimeType="application/binary"/>
            <mimeMap fileExtension=".memgz" mimeType="application.binary"/>
        </staticContent>
    </system.webServer>
    <system.web>
        <httpRuntime maxRequestLength="4096" targetFramework="4.5" enableVersionHeader="false"/>
    </system.web>
    <system.data>
    </system.data>
</configuration>
